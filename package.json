{
  "name": "@warren-bank/lz4-wasm",
  "version": "1.0.0",
  "description": "LZ4 block format encoder/decoder: a WebAssembly implementation with fallback to a pure JavaScript implementation.",
  "license": "BSD-2-Clause",
  "exports": {
    ".": {
      "browser": "./dist/web/lz4-block-codec-any.js",
      "import":  "./dist/esm/lz4-block-codec-any.mjs",
      "require": "./dist/cjs/lz4-block-codec-any.cjs"
    },
    "./js": {
      "browser": "./dist/web/lz4-block-codec-js.js",
      "import":  "./dist/esm/lz4-block-codec-js.mjs",
      "require": "./dist/cjs/lz4-block-codec-js.cjs"
    },
    "./wasm": {
      "browser": "./dist/web/lz4-block-codec-wasm.js",
      "import":  "./dist/esm/lz4-block-codec-wasm.mjs",
      "require": "./dist/cjs/lz4-block-codec-wasm.cjs"
    }
  },
  "files": [
    "dist/"
  ],
  "author": {
    "name": "Warren R. Bank",
    "email": "warren.r.bank@gmail.com",
    "url": "https://github.com/warren-bank"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/warren-bank/node-lz4-wasm"
  },
  "keywords": [
    "lz4"
  ]
}
